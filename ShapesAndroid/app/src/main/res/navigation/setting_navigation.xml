<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_settings">

    <fragment
        android:id="@+id/navigation_saved"
        android:name="com.example.myapplication.ui.saved.SavedFragment"
        android:label="@string/title_saved"
        tools:layout="@layout/fragment_profile" />

    <fragment
        android:id="@+id/navigation_liked"
        android:name="com.example.myapplication.ui.liked.LikedFragment"
        android:label="@string/liked"
        tools:layout="@layout/fragment_profile" />

    <fragment
        android:id="@+id/navigation_logout"
        android:name="com.example.myapplication.ui.logout.LogoutFragment"
        android:label="@string/logout"
        tools:layout="@layout/fragment_profile" />
</navigation>