<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_home">

    <fragment
        android:id="@+id/nav_home"
        android:name="com.example.myapplication.ui.home.HomeFragment"
        android:label="@string/home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/nav_explore"
        android:name="com.example.myapplication.ui.explore.ExploreFragment"
        android:label="@string/explore"
        tools:layout="@layout/fragment_explore" />

    <fragment
        android:id="@+id/nav_people"
        android:name="com.example.myapplication.ui.people.PeopleFragment"
        android:label="@string/people"
        tools:layout="@layout/fragment_people" />

    <fragment
        android:id="@+id/nav_add_post"
        android:name="com.example.myapplication.ui.upload.UploadPostFragment"
        android:label="@string/upload"
        tools:layout="@layout/fragment_add_post" />

    <fragment
        android:id="@+id/nav_filter"
        android:name="com.example.myapplication.ui.filter.FilterFragment"
        android:label="@string/filter"
        tools:layout="@layout/fragment_filter"/>




</navigation>
